@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityRole>
@{
    ViewData["Title"] = "Application Roles";
}
<h1>View Roles</h1>
<a asp-action="Create">Create</a>
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
       <tr>
           <td>@role.Id</td>
           <td>@role.Name</td>
           </tr>
            
        }
    </tbody>
</table>